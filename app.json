      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/app-icon-web-favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a84e02ca-76df-46a1-813a-87310e43be04"
      }
    }
