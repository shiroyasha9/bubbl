{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./app",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "react-native",
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "paths": {
      "@components": ["components"],
      "@navigators": ["navigators"],
      "@config/*": ["config/*"],
      "@services": ["services"],
      "@services/*": ["services/*"],
      "@styles": ["styles"],
      "@styles/*": ["styles/*"],
      "@theme": ["theme"],
      "@utils": ["utils"],
      "@utils/*": ["utils/*"],
      "@models": ["models"],
      "@assets": ["../assets/*"],
      "@screens": ["screens"],
      "@constants": ["constants"],
      "@types": ["types"]
    },
    "sourceMap": true,
    "target": "esnext",
    "lib": ["esnext", "dom"],
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "exclude": ["node_modules"],
  "include": ["App.js", "app", "storybook"],
  "extends": "expo/tsconfig.base"
}
